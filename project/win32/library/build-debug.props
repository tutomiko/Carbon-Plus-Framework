<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <OutDir>$(SolutionDir)\lib</OutDir>
    <TargetName>$(SolutionName)-$(PlatformTarget) (Debug)</TargetName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <CompileAs>CompileAsCpp</CompileAs>
      <AdditionalIncludeDirectories>$(SolutionDir)/include;$(SolutionDir)/resources/include</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>_CARBONPLUS_SELF;_CARBONPLUS_STATIC_LINKAGE;_CARBONPLUS_ENABLE_DEPRECATED_FEATURES;_CARBONPLUS_ENABLE_EXPERIMENTAL_FEATURES;_WINSOCK_DEPRECATED_NO_WARNINGS;_CRT_SECURE_NO_WARNINGS;_UNICODE;UNICODE;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <PrecompiledHeaderFile>pch.h</PrecompiledHeaderFile>
      <PrecompiledHeader>NotUsing</PrecompiledHeader>
      <RuntimeLibrary>MultiThreadedDebug</RuntimeLibrary>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>